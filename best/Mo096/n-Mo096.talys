ldmodel 2
#
# (n,tot), (n,el), (n,inl)
#
#
# (n,p), (n,2n), (n,np)
#
rvadjust p 1.13
avadjust p 1.13
gnadjust 42 97 0.98
gpadjust 42 97 0.98
#
rvadjustF p 12 35 -50 2.0
gnadjust 41 96 0.85
gpadjust 41 96 0.85
