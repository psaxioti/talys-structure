#
# General
#
ldmodel 2
#
# (n,tot), (n,el), (n,inl)
#
#
# (n,p), (n,2n), (n,np)
#
#
# (n,a)
#
rvadjust d 0.75
avadjust d 0.75
cstrip a 0.20
cknock a 0.20
#
# (n,g)
#
gamgamadjust  6  13 0.10                                                        
#
# Other: Isomers, (n,d), (n,t), (n,h) etc.
#
rvadjust d 0.85
avadjust d 0.85
cstrip d 0.60
cknock d 0.60
rvadjust t 0.85
avadjust t 0.85
cstrip t 0.40
cknock t 0.40
