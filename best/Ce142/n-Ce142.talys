# General
#
ldmodel 2
#
# (n,tot), (n,el), (n,inl)
#
gpadjust 58 143 1.25
gnadjust 58 143 1.25
#
# (n,p), (n,2n), (n,np)
#
gpadjust 57 142 1.3
gnadjust 57 142 1.3
rvadjust p 1.27
avadjust p 1.27
#
# (n,a)
#
alphaomp 6
rvadjust a 1.03
avadjust a 1.03
#
# (n,g)
#
gamgamadjust 58 143 1.5
rgamma 6.2
#
# Other: Isomers, (n,d), (n,t), (n,h) etc.
#
rvadjust h 0.8
avadjust h 0.8
